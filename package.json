{
  "name": "markdown-it-plugins",
  "version": "0.0.3",
  "packageManager": "pnpm@6.32.3",
  "description": "",
  "keywords": [
    "markdown-it",
    "plugins"
  ],
  "homepage": "https://github.com/banlify/markdown-it-plugins#readme",
  "bugs": {
    "url": "https://github.com/banlify/markdown-it-plugins/issues"
  },
  "license": "MIT",
  "author": "Chestnut <banlify@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banlify/markdown-it-plugins.git"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild && nr minify",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "minify": "zx ./scripts/minify.mjs",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.5",
    "@antfu/ni": "^0.13.2",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/node": "^17.0.21",
    "@types/uglify-js": "^3.13.1",
    "bumpp": "^7.1.1",
    "eslint": "^8.10.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "markdown-it": "^12.3.2",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "typescript": "^4.6.2",
    "uglify-js": "^3.15.3",
    "uglifycss": "^0.0.29",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "zx": "^6.0.6"
  },
  "dependencies": {
    "markdown-it-container": "^3.0.0"
  }
}
